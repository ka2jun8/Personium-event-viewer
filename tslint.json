{
    "rules": {
        "quotemark": [true, "double", "avoid-escape"],
        "indent": [true,"spaces"],
        "no-eval": true,
        "triple-equals": [true, "allow-null-check"],
        "class-name": true,
        "curly": true,
        "forin": true,
        "no-var-keyword": true,
        "no-console": [false, "log"]
    }
}